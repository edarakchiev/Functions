def multiplication(num_1, num_2, num_3):
    result = num_1 * num_2 * num_3
    if result < 0:
        return "negative"
    elif result > 0:
        return "positive"
    else:
        return "zero"


num_1 = int(input())
num_2 = int(input())
num_3 = int(input())

print(multiplication(num_1, num_2, num_3))
