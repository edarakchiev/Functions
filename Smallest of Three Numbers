def smallest_number(num_1, num_2, num_3):
    if num_1 < num_2 and num_1 < num_3:
        return num_1
    elif num_2 < num_1 and num_2 < num_3:
        return num_2
    elif num_3 < num_1 and num_3 < num_1:
        return  num_3


num_1 = int(input())
num_2 = int(input())
num_3 = int(input())

min_num = smallest_number(num_1, num_2, num_3)
print(min_num)
